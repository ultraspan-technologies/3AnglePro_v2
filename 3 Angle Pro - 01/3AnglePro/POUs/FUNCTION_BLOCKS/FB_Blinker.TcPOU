  <POU Name="FB_Blinker" Id="{72ea74e8-2cef-41b1-82ab-473c3b1b5b05}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_Blinker
VAR_INPUT
	IN			:	BOOL;
	blink_time	:	TIME;
END_VAR
VAR_OUTPUT
	OUT			:	BOOL;
END_VAR
VAR
	timerON		:	TON;
	timerOFF	:	TOF;
END_VAR

]]></Declaration>
    <Implementation>
      <ST><![CDATA[timerON(IN:= 	IN AND NOT timerOFF.Q,
