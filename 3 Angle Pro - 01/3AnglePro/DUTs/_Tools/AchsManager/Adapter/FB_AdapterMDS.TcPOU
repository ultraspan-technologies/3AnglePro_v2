<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.15">
  <POU Name="FB_AdapterMDS" Id="{c74eeab7-3446-43f1-9871-dd6edd04cf50}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_AdapterMDS
VAR_INPUT
	DriveAdapter_from_Achs: TAchs_To_DriveAdapter;
	///Maximaldrehzahl in Upm für Skalierung.durch Angabe eines negativen Wertes wird der Drehzahlsollwert invertiert 
	nMax_Motor: REAL := 3000.0;
	///Impulse / Motroumdrehung des Motorgebers
	IncPerTurnEngine: DWORD;
END_VAR
VAR_IN_OUT
	///Zur Koppelung zum AchsManager
	AxModule: TAxModule;
	///Das Laderelais muss für min 12s geschlossen sein um die Treiberschütze zuzuschalten
	LoadRelayMDSClosed: BOOL;
	///Verknüpfung zur PDO_IN Struktur der Achse
	PDO_IN: stPDO_MDS_IN;
	///Verknüpfung zur PDO_OUT Struktur der Achse
	PDO_OUT: stPDO_MDS_OUT;
END_VAR
VAR_OUTPUT
	///Enable Inverter
	ENBL: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[_EnblAxis();

PDO_OUT.D230_nSoll:=REAL_TO_INT(LIMIT(-16384,(16384*DriveAdapter_from_Achs.motorSpeed/nMax_Motor),16384));

_WriteAxState();
]]></ST>
    </Implementation>
