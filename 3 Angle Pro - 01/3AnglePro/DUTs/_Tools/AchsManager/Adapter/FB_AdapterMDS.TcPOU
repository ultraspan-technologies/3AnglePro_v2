    <Action Name="_WriteAxState" Id="{acdbc983-b254-4b46-b5e3-8afc90e59cdb}">
      <Implementation>
        <ST><![CDATA[

AxModule.AxState.AktCurrent:=0;	//[%Nenn]
AxModule.AxState.AktTorque:=0;	//[%M-Max]
AxModule.AxState.AktVel:=PDO_IN.E91_v_Motor;	//[rpm]
AxModule.AxState.AktUZk:=0;	//[V]

AxModule.AxState.Errorcause:=SHL(PDO_IN.P82_Error,8)AND 16#00FF;
AxModule.AxState.ErrorCode:=PDO_IN.P82_Error AND 16#00FF;

(* Inverter State *)
AxModule.AxState.InverterState.VoltageEnabled := TRUE; //Keine Rückmeldung bei MDS
AxModule.AxState.InverterState.InverterError := PDO_IN.E200_DeviceState.1;
AxModule.AxState.InverterState.SafetyModuleError := FALSE;
AxModule.AxState.InverterState.InverterWarning := FALSE;
AxModule.AxState.InverterState.QuickstopActive := PDO_IN.E200_DeviceState.2;
AxModule.AxState.InverterState.AspOk := TRUE; //Keine Rückmeldung bei MDS
			
AxModule.AxState.InverterState.EncoderError:=(PDO_IN.P82_Error AND 16#00FF)=37;

IF PDO_IN.P10_State.1 THEN
	LoadRelayMDSClosed:=FALSE;
END_IF]]></ST>
      </Implementation>
    </Action>
